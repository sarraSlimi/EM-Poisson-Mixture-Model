\name{log_likelihood}
\alias{log_likelihood}
\title{
  Calculate Log-Likelihood of Data in Poisson Mixture Model
}
\usage{
  \code{log_likelihood(model, X)}
}
\arguments{
  \item{model}{A list containing the parameters of the Poisson mixture model.}

  \item{X}{Data matrix.}
}
\description{
  Calculates the log-likelihood of the data given the parameters of a Poisson mixture model.
}
