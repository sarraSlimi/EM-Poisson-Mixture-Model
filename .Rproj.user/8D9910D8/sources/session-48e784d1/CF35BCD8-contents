\name{initialize_model}
\alias{initialize_model}
\title{
  Initialize Poisson Mixture Model Parameters
}
\usage{
  \code{initialize_model(num_clusters, num_features)}
}
\arguments{
  \item{num_clusters}{Number of clusters in the model.}

  \item{num_features}{Number of features in the data.}
}
\description{
  Initializes parameters for a Poisson mixture model.
}
