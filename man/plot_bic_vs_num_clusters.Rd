\name{plot_bic_vs_num_clusters}
\alias{plot_bic_vs_num_clusters}
\title{
  Plot Bayesian Information Criterion (BIC) vs. Number of Clusters
}
\usage{
  \code{plot_bic_vs_num_clusters(num_clusters_range, bics)}
}
\arguments{
  \item{num_clusters_range}{Range of the number of clusters considered.}

  \item{bics}{Vector of BIC values corresponding to each number of clusters.}
}
\description{
  Plots the Bayesian Information Criterion (BIC) against the number of clusters.
}
